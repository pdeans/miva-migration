{
    "name": "pdeans/miva-migration",
    "description": "MVC framework application skeleton for Miva data migrations and integrations.",
    "keywords": ["Miva migration", "Miva integration", "Miva", "data migration", "migration", "data integration", "integration"],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Patrick Stearns",
            "email": "pdeans1986@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/database": "^5.4",
        "pdeans/miva-provision": "^2.1",
        "php-di/slim-bridge": "^1.0",
        "slim/slim": "^3.0",
        "slim/twig-view": "^2.2",
        "vlucas/phpdotenv": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}